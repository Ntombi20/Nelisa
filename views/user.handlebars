<h1>Users</h1>
<h3><a href="/user/add">Add a new users</a></h3>
<table class="table">
    <tr>
        <th>Username</th>
        <th>Password</th>
        <th>Email</th>
        <th>Roles</th>
    </tr>
    {{#users}}
        <tr>
            <td data-th="Username">{{username}}</td>
            <td data-th="Password">{{password}}</td>
            <td data-th="Email">{{email}}</td>
            <td data-th="Roles">{{admin}}</td>
        </tr>
    {{/users}}
</table>
